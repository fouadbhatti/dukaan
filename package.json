    "webpack-pwa-manifest": "^3.6.2",
    "workbox-webpack-plugin": "^3.4.1"
  },
  "scripts": {
    "development": "webpack-dev-server  --mode=development --progress --colors --open --hot",
    "spin:up": "webpack-dev-server --mod=production --host 0.0.0.0 --open",
    "build": "rimraf dist/* && webpack --mode=production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:snapshot:update": "npm run test -- -u",
    "test:watch": "npm run test -- --watch",
    "test:e2e": "start-server-and-test spin:up http://localhost:8090 e2e",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "lint": "eslint .",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:status": "flow status",
    "flow:coverage": "flow coverage",
    "ci": "npm run test & npm run test:coverage & npm run test:e2e"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-test-framework-setup.js",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/fbjs"
    ],
    "roots": [
      "<rootDir>/__tests__/unit:integration/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
