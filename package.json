    "prettier-eslint": "^8.8.2",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.3",
    "start-server-and-test": "^1.5.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1",
